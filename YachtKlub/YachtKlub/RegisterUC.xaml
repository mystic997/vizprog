<UserControl x:Class="YachtKlub.RegisterUC"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
             xmlns:local="clr-namespace:YachtKlub"
             mc:Ignorable="d" 
             d:DesignHeight="600" d:DesignWidth="900">
    <Grid>
        <Button x:Name="btHajo" Content="Hajó regisztrálása" HorizontalAlignment="Left" Margin="100,250,0,0" VerticalAlignment="Top" Width="300" Height="100" Click="btHajo_Click"/>
        <Button x:Name="btUser" Content="Felhasználó regisztrálása" HorizontalAlignment="Right" Margin="0,250,100,0" VerticalAlignment="Top" Width="300" Height="100" Click="btUser_Click"/>

    </Grid>
</UserControl>
